d() {
	?? $# = 0 && cd /home/code && return
	ff d ${1/\//*/} /home/code && cd $match
}

dw() {
	?? $# = 0 && d w && return
	d w/$1
}

alias db='build'
alias dbd='build-data'

di() {
	ni $@ || return 1
	local dir=`npm prefix`
	mkdir -p $dir/run/lib
	mv $dir/node_modules/* ${dir}/run/lib
	rm -r ${dir}/node_modules
	cd $dir
}

dd() {
	?? $# = 1 && d $1
	vite
}
