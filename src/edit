alias e='nvim'

eff() {
	[ $# -eq 1 ] && { cd $1; cr; return 0; }
	
	ff f $2 $1		\
		&& e $match	\
		|| cd $1
}

alias ec='eff ~f/cfg'
alias eb='eff ~f/bin'
alias er='eff ~f/run'
alias eC='eff ~/.config'

es() {
	eff ~f/src $1
	[ ! -z $match ] && {
		source $match  
		cat $match > ~f/run/spool
	}
}
